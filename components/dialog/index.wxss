.dialog-container {
  width: 340rpx;
  height: 210rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  background: #000;
  border-radius: 20rpx;
  opacity: .7;
  color: #fff;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.tip-img {
  width: 10rpx;
  height: 46rpx;
  margin-bottom: 38rpx;
}